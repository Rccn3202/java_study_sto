package ch02.sec01;

public class s {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		 int x, y, sum;

		    Scanner scan = new Scanner(System.in);

		    System.out.println("숫자 입력:");

		    x = scan.nextInt();



		    System.out.println("숫자 입력:");

		    y = scan.nextInt();



		    sum = x + y;

		    System.out.println("합계: " + sum);

		    scan.close();
		
		
	}

}
